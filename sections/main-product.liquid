    .c-product-usps__title {
        margin-bottom: 1.75rem
    }

    .c-product-usps__block {
        width: auto
    }

    .c-product-usps__block-title {
        margin-top: .5rem
    }
}

@media only screen and (min-width: 1650px) {
    .c-product-usps {
        padding: 2rem 10.5rem
