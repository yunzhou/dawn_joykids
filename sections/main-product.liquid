<style type="text/css">
    .c-product-usps {
    padding: 2rem 1rem .25rem;
    border-top: 1px solid #E5E5E5;
    border-bottom: 1px solid #E5E5E5
}

.c-product-usps__title {
    text-align: center;
    margin: 0 0 1.5rem
}

.c-product-usps__blocks {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-around
}

.c-product-usps__block {
    width: 44%;
    margin-bottom: 1.75rem
}

.c-product-usps__block-icon {
    width: 3.375rem;
    height: 3.375rem;
    margin: auto
}

.c-product-usps__block-icon svg {
    width: 100%;
    height: 100%;
    transform: none
}

.c-product-usps__block-title {
    text-align: center;
    margin: .25rem auto 0;
    max-width: 10rem
}

